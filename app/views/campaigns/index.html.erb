<div class="row">
  <div class="col s12">
    <ul class="tabs">
      <li class="tab col s2"><a href="#test1">ALL</a></li>
      <li class="tab col s2"><a href="#test2">EDUCATION</a></li>
      <li class="tab col s2"><a href="#test3">SPORT</a></li>
      <li class="tab col s2"><a href="#test4">MEDICINE</a></li>
      <li class="tab col s2"><a href="#test5">FOOD</a></li>
      <li class="tab col s2"><a href="#test6">PETS</a></li>
    </ul>
  </div>
  <div id="test1" class="col m12">
      <div class="row">
        <% @campaigns.each do |campaign| %>
            <% if campaign.goal>=campaign.collection(campaign.id)  %>
              <div class="col s4">
                  <div class="card">
                    <div class="card-image waves-effect waves-block waves-light">
                      <%= image_tag campaign.avatar.url(:medium),class: "size" %>
                    </div>
                    <div class="card-content">
                      <center>
                        <span class="card-title activator grey-text text-darken-4"><%= campaign.title %><i class="material-icons right">more_vert</i></span>
                      </center>
                      <%= link_to 'View',campaign,class:"modal-action modal-close modal-action waves-effect waves-light btn" %>
                    </div>
                    <div class="card-reveal">
                      <span class="card-title grey-text text-darken-4"><%= campaign.title %><i class="material-icons right">close</i></span>
                      <p>Goal: <%= campaign.goal %></p>
                      <p>Collection</p>
                      <div class="progress">
                        <div class="determinate" style="width: <%= campaign.progress(campaign.id) %>%"></div>
                      </div>
                      <p>Created at: <%= campaign.created_at %></p>
                      <p>Created by: <%= campaign.user.name %></p>
                    </div>
                  </div>
                </div>
            <% end %>
        <% end %>
      </div>
  </div>
